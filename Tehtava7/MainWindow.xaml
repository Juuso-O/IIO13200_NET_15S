<Window x:Class="Tehtava7.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Tehtava7"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <StackPanel>
            <ComboBox x:Name="cmbPlaces" Margin="2,2,2,2" />
            <Button x:Name="btnGet" Margin="2,2,2,2" Content="Hae" Click="btnGet_Click"/>
        </StackPanel>
        <DataGrid x:Name="dgData" Margin="10,65,10,10" ItemsSource="{Binding}" />
    </Grid>
</Window>
