<Window x:Class="Tehtava5.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Tehtava5"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525" ResizeMode="NoResize">
    <Grid>
        <Button x:Name="btnGetDataTable" Content="Hae läsnäolot (DataTable)" Margin="10,10,10,280" Click="btnGetDataTable_Click" />
        <TextBox x:Name="txtBox" Margin="10,45,10,241" />
        <Button x:Name="btnGetDataView" Content="Hae läsnäoloni (DataView)" Margin="10,84,10,206" Click="btnGetDataView_Click" />
        <Button x:Name="btnGetDataSet" Content="Hae läsnäolot (Typed DataSet)" Margin="10,119,10,171" Click="btnGetDataSet_Click" />
        <DataGrid x:Name="dgData" Margin="10,154,10,10" ItemsSource="{Binding}">

        </DataGrid>
    </Grid>
</Window>
